<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="JobTitleDao" table="EMPLOYEE">
        <meta attribute="class-description">
            This class contains the job title detail.
        </meta>
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="jobTitle" column="title" type="string"/>
        <property name="lowSalary" column="low_salary" type="java.lang.Integer"/>
        <property name="highSalary" column="high_salary" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>